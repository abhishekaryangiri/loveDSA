// Syntax: 

//  switch(variable) {
//  case 1 :
//  case 2 :
//  case 3 :
//	  default :
//  }

// it will compare cases with variable-> if cond true then print
package loveDSA;

public class $10Switch {
	public static void main(String[] args) {
		int num = 2;
		switch( num) {
		case 1 : System.out.println("Bangalore");
		break;
		case 2 : System.out.println("Delhi");
		break;
		case 3 : System.out.println(" Puna");
		break;
		case 4 :  System.out.println("Mumbai");
		break;
		default : System.out.println("USA");
		break;
		}
	}

}
