package loveDSA;

import java.util.Scanner;

public class $12BreakUsingScanner {
	public static void main(String[] args) {
		System.out.println(" Enter Number");
		Scanner abhi = new Scanner(System.in);
		do {
			int ui =	abhi.nextInt();
			if( ui % 10 ==0)
				break;
			System.out.println(ui);
	}
		while (true);
	}
}
