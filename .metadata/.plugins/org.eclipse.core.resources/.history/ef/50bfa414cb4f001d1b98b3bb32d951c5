package loveDSA;

import java.util.Scanner;

public class $11ProjectCal {
	Scanner abhi = new Scanner(System.in);
	int a = abhi.nextInt();
	char operator = abhi.next().charAt(0);
	int b = abhi.nextInt();
	
	switch (operator) {
	case "+" : System.out.println(a + b);
	break;
	case "-" : System.out.println(a-b);
	break;
	case "*" : System.out.println(a*b);
	break;
	case "/" : System.out.println(a/b);
	break;
	case "%" : System.out.println(a%b);
	default : System.out.println("Error 404");
	
				
	}

}
