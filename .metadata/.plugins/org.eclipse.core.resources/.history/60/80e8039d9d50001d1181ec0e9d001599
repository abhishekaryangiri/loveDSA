package loveDSA;

public class $2PrintNum1to20 {
	public static void main(String[] args) {
		 int start = 1;
		 while( start <= 20) {
			 System.out.println(start);
			 start++;
		 }
		 //print table of 2
		 int s = 1;
		 while( s<=10) {
			 System.out.println("table of 2 is given below");
			 System.out.println(s*2);
			 s++;
		 }
	}
}
