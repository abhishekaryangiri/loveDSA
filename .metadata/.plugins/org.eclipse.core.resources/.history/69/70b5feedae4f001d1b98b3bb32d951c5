// if, else | else if | ternamry operator | switch
// if-else: if false then else will work
// paranthesis -> ()
//  curly braces -> {}
package loveDSA;

public class $1IfElse {
public static void main(String[] args) {
	int age = 18;
	if (  age >= 18) {
		System.out.println("Eligible for vote");
	}else {
		System.out.println("age is not enought for vote");
	}
}
}
